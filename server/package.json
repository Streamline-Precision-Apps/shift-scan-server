{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--import ./src/instrument.mjs\" tsx watch src/index.ts",
    "build": "tsc && npm run sentry:sourcemaps",
    "start": "node --import ./src/instrument.mjs dist/index.js",
    "docker:deploy": "./deploy-to-cloudrun.sh",
    "docker:deploy-win": "bash ./deploy-to-cloudrun.sh",
    "db:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prisma:generate": "npx dotenv-cli -e .env -- npx prisma generate",
    "prisma:push": "npx dotenv-cli -e .env -- npx prisma db push",
    "prisma:migrate": "npx dotenv-cli -e .env -- npx prisma migrate dev",
    "prisma:migrate deploy": "npx dotenv-cli -e .env -- npx prisma migrate deploy",
    "prisma:studio": "npx dotenv-cli -e .env -- npx prisma studio",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org streamline-precision-llc-el --project javascript-nextjs ./dist && sentry-cli sourcemaps upload --org streamline-precision-llc-el --project javascript-nextjs ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.18.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@sentry/cli": "^2.57.0",
    "@sentry/node": "^10.22.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^10.0.0",
    "express": "^5.1.0",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "next-auth": "^5.0.0-beta.29",
    "resend": "^6.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
